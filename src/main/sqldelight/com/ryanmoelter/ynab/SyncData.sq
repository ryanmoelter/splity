import co.moelten.splity.database.AccountId;
import co.moelten.splity.database.BudgetId;


CREATE TABLE syncData (
  firstServerKnowledge INTEGER NOT NULL,
  firstBudgetId TEXT AS BudgetId NOT NULL,
  firstAccountId TEXT AS AccountId NOT NULL,
  firstAccountFetchedName TEXT NOT NULL,
  secondServerKnowledge INTEGER NOT NULL,
  secondBudgetId TEXT AS BudgetId NOT NULL,
  secondAccountId TEXT AS AccountId NOT NULL,
  secondAccountFetchedName TEXT NOT NULL
);

getOnly:
SELECT *
FROM syncData
;
