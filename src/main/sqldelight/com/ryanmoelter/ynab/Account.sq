import com.youneedabudget.client.models.Account;
import java.util.UUID;
import kotlin.Boolean;

CREATE TABLE account (
  id TEXT AS UUID PRIMARY KEY NOT NULL,
  name TEXT NOT NULL,
  type TEXT AS Account.TypeEnum,
  onBudget INTEGER AS Boolean NOT NULL,
  closed INTEGER AS Boolean NOT NULL,
  transferPayeeId TEXT AS UUID NOT NULL,
  deleted INTEGER AS Boolean NOT NULL,
  budgetId TEXT AS UUID NOT NULL,
  FOREIGN KEY(budgetId) REFERENCES budget(id)
);

CREATE INDEX accountId ON account(id);
